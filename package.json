{
	"name": "custom kafka module",
	"description": "Custom kafka module",
	"version": "1.0.0",
	"scripts": {
		"build": "rimraf -rf dist && tsc -p tsconfig.json",
		"prepublish:npm": "npm run build",
		"publish:npm": "release-it",
		"prepublish:dev": "npm run build",
		"publish:dev": "npm publish --access public --tag dev",
		"prepare": "husky install .github/husky",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint --ignore-path .gitignore {integration,src}/**/*.ts",
		"test": "jest"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"author": "Maxim Filippov",
	"license": "MIT",
	"keywords": [],
	"dependencies": {
		"@nestjs/common": "^10.3.10",
		"@nestjs/core": "^10.3.10",
		"@nestjs/microservices": "^10.3.10",
		"kafkajs": "^2.2.4",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.1"
	},
	"devDependencies": {
		"@commitlint/cli": "19.2.1",
		"@commitlint/config-angular": "19.1.0",
		"@types/jest": "29.5.12",
		"@types/node": "20.14.5",
		"@typescript-eslint/eslint-plugin": "5.62.0",
		"@typescript-eslint/parser": "5.62.0",
		"eslint": "8.57.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-import": "2.29.1",
		"husky": "9.0.11",
		"jest": "29.7.0",
		"prettier": "3.2.5",
		"release-it": "17.1.1",
		"rimraf": "5.0.5",
		"ts-jest": "29.1.2",
		"typescript": "5.4.2"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/MaxOF/custom-kafka-module/"
	},
	"homepage": "https://github.com/MaxOF/custom-kafka-module/#readme"
}
